h1.title-section Input Forms
.gs-wrap
  //-.old
    p Input Text
    .input
      input(type="text", placeholder="Enter your value")
    p Input Number
    .input
      input(type="number", placeholder="Enter a number")
    p Input Radio
    .input
      input(type="radio", placeholder="Enter your value")
    p Input checkbox
    .input
      input(type="checkbox", placeholder="Enter your value")
    p Input Submit
    .input
      input(type="submit", value="Input Submit")
    p Icon Button
    .input
      button Button
        i.icon-cancel
    .input
      button Button
        i.icon-check

