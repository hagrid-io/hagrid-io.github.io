mixin mainDropdowns
  div.code-example
    h6 Code:
    pre
      code(class="language-html", prism)
        :code
          <div class="dropdown dropdown-primary" hagrid-component="dropdown">
            <a href="#" class="btn btn-primary" hagrid-role="dropdown">Dropdown A<span class="icon-dropdown icon-keyboard_arrow_down"></span></a>
            <ul class="dropdown-menu">
              <li><a href="#">Dropdown Menu 1</a></li>
              <li><a href="#">Dropdown Menu 2</a></li>
              <li><a href="#">Dropdown Menu 3</a></li>
              <li><a href="#">Dropdown Menu 4</a></li>
              <li><a href="#">Dropdown Menu 5</a></li>
            </ul>
          </div>

  div.code-result
    h6 Result:
    .dropdown.dropdown-primary(hagrid-component='dropdown')
      a.btn.btn-primary(href='#', hagrid-role='dropdown')
        | Dropdown A
        span.icon-dropdown.icon-keyboard_arrow_down
      ul.dropdown-menu
        li
          a(href='#') Dropdown Menu 1
        li
          a(href='#') Dropdown Menu 2
        li
          a(href='#') Dropdown Menu 3
        li
          a(href='#') Dropdown Menu 4
        li
          a(href='#') Dropdown Menu 5

