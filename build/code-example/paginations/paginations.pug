mixin mainPaginations
  div.code-example
    h6 Code: 
    pre
      code(class="language-html", prism)
        :code
          <nav class="pagination">
            <a href="">1</a>
            <a href="">2</a>
            <a href="">3</a>
            <a href="">4</a>
          </nav>
          <nav class="pagination">
            <span class="pagination-prev disable"></span>
            <a href="">1</a>
            <a href="">2</a>
            <a href="">3</a>
            <a href="">4</a>
            <span class="pagination-next"></span>
          </nav>
          <nav class="pagination">
            <span class="pagination-prev"></span>
            <a href="">3</a>
            <a href="" class="current">4</a>
            <span class="ellipsis"></span>
            <a href="">8</a>
            <a href="">9</a>
            <span class="pagination-next"></span>
          </nav>
          

  div.code-result
    h6 Result:
    nav.pagination
      a(href='') 1
      a(href='') 2
      a(href='') 3
      a(href='') 4
    nav.pagination
      span.pagination-prev.disable
      a(href='') 1
      a(href='') 2
      a(href='') 3
      a(href='') 4
      span.pagination-next
    nav.pagination
      span.pagination-prev
      a(href='') 3
      a.current(href='') 4
      span.ellipsis
      a(href='') 8
      a(href='') 9
      span.pagination-next


